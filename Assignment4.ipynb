    "import numpy as np\n",
    "\n",
    "X = np.array([2, 4, 6, 8, 10])\n",
    "Y = np.array([1, 3, 2, 5, 7])\n",
    "\n",
    "#coveriance matrcic\n",
    "cov_matrix = np.cov(X, Y)\n",
    "\n",
    "#get covariance value from matrix\n",
    "cov_XY = cov_matrix[0, 1]\n",
    "\n",
    "print(\"Covariance:\", cov_XY)"
   ]
  }
